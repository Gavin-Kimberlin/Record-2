<h1>Add a new album</h1>

<form action="/albums" method="post">
  <div class="form-group">
    <label for="album_name">Album name</label>
    <input id="n" name="album_name" class="form-control" type="text">
    <label for="album_name">Album Year</label>
    <input id="n" name="album_year" class="form-control" type="text">
    <label for="album_name">Artist</label>
    <input id="n" name="album_artist" class="form-control" type="text">
    <label for="album_name">Genre</label>
    <input id="n" name="album_genre" class="form-control" type="text">
  </div>
  <button type="submit" class="btn">Go!</button>
</form>
